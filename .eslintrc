{
  "plugins": ["vue"],
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "plugin:vue/essential",
    "prettier",
    "@vue/prettier"
  ],
  "root": true,
  "env": {
    "node": true
  },
  "rules": {
    "console": true,
    "vue/max-attributes-per-line": 0,
    "vue/no-unused-vars": "warning"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
